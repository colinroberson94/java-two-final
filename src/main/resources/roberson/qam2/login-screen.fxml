<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="400.0" maxWidth="600.0" minHeight="250.0" minWidth="400.0" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreenController">
   <children>
      <Label fx:id="userLocation" layoutX="14.0" layoutY="14.0" text="Location" />
      <TextField fx:id="usernameField" layoutX="94.0" layoutY="95.0" prefHeight="26.0" prefWidth="208.0" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="95.0" layoutY="125.0" prefHeight="26.0" prefWidth="208.0" promptText="Password" />
      <Button fx:id="submitLoginButton" layoutX="209.0" layoutY="162.0" mnemonicParsing="false" onAction="#onActionSubmit" text="Submit" />
      <Button fx:id="cancelLoginButton" layoutX="134.0" layoutY="161.0" mnemonicParsing="false" onAction="#onActionCancel" prefHeight="26.0" prefWidth="56.0" text="Cancel" />
   </children>
</AnchorPane>
